<script src="../js/jquery-3.3.1.js"></script>
<script src="../js/bootstrap.min.js"></script>
<script>
 
    //用ajax提交表单
    $('#sugges').click(function () {
        var username=$('#user').val();
        var password=$('#password').val();
        var age=$('#age').val();
        var sex=$('#sex').val();
        var hasGirlfriendorBoyfriend=$('#hasGirlfriendorBoyfriend').val();
          $.ajax({
                type: "post",
                url: "/user/add",
                dataType: "json",
                data:{ "user": username, "password": password ,"hasGirlfriendorBoyfriend":hasGirlfriendorBoyfriend,"sex":sex,"age":age},
                success: function(data){
                //接受返回的数据，前端判断采取的动作
                  if(data){
                    if(data.message=="false"){
                      alert('注册失败');
                    }else{
                      alert("注册成功");  
                    }
                  }else{
                     alert('注册失败');
                  }
                }
          });
            }); 
  

</script>
</html> 
